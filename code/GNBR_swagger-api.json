{
  "swagger" : "2.0",
  "info" : {
    "description" : "This API provides access to the relationships between entities in the global network of biomedical relationships (GNBR) derived from text",
    "version" : "1.0.0",
    "title" : "GNBR",
    "contact" : {
      "email" : "alavertu@stanford.edu"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "tags" : [ {
    "name" : "admins",
    "description" : "Secured Admin-only calls"
  }, {
    "name" : "developers",
    "description" : "Operations available to regular developers"
  } ],
  "paths" : {
    "/find_entity" : {
      "get" : {
        "tags" : [ "developers" ],
        "summary" : "searches entities within GNBR for a matching ID, based on input string",
        "operationId" : "getIdentifier",
        "description" : "Search for entity ids with GNBR\n",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "query",
          "name" : "searchString",
          "description" : "pass a search string to find matching identifiers",
          "required" : true,
          "type" : "string"
        }, {
          "in" : "query",
          "name" : "limit",
          "description" : "maximum number of records to return",
          "type" : "integer",
          "format" : "int32",
          "minimum" : 0,
          "maximum" : 10
        } ],
        "responses" : {
          "200" : {
            "description" : "search results matching criteria",
            "schema" : {
              "$ref" : "#/definitions/idMapping"
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/query_edge" : {
      "get" : {
        "tags" : [ "developers" ],
        "summary" : "Query for an edge",
        "operationId" : "getEdge",
        "description" : "Query for edges connecting two entities within GNBR",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "query",
          "name" : "entity1",
          "description" : "GNBR-ID for first entity",
          "type" : "string"
        }, {
          "in" : "query",
          "name" : "entity2",
          "description" : "GNBR-ID for second entity",
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "search results matching criteria",
            "schema" : {
              "$ref" : "#/definitions/idMapping"
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    }
  },
  "definitions" : {
    "idMapping" : {
      "type" : "object",
      "required" : [ "common_name", "GNBR_ID", "is_medic" ],
      "properties" : {
        "common_name" : {
          "type" : "string",
          "example" : "rosuvastatin"
        },
        "GNBR_entity" : {
          "$ref" : "#/definitions/gnbrEntity"
        },
        "is_medic" : {
          "type" : "boolean",
          "example" : true
        }
      }
    },
    "gnbrEntity" : {
      "required" : [ "name" ],
      "properties" : {
        "entity_ID" : {
          "type" : "string",
          "example" : "MESH:D000068718"
        },
        "synonyms" : {
          "type" : "array",
          "example" : [ "Rosuvastatin", "Crestor", "Calcium, Rosuvastatin" ]
        },
        "entity_type" : {
          "type" : "string",
          "example" : "chemical"
        }
      }
    },
    "gnbrEdge" : {
      "required" : [ "edge_type", "edge_score", "sentence", "pubmed_id" ],
      "properties" : {
        "edge_type" : {
          "type" : "string",
          "example" : "increases expression/production"
        },
        "edge_score" : {
          "type" : "number",
          "example" : 12.5
        },
        "sentence" : {
          "type" : "string",
          "example" : "A Phase I trial of a potent P-glycoprotein inhibitor, zosuquidar_trihydrochloride [LY335979], administered intravenously in combination with doxorubicin in patients with advanced malignancy."
        },
        "pubmed_id" : {
          "type" : "string",
          "example" : "pubmed:23411283"
        }
      }
    },
    "gnbrAssociation" : {
      "required" : [ "subject_id", "object_id", "association_score" ],
      "properties" : {
        "subject_id" : {
          "type" : "string",
          "example" : "MESH:D000068718"
        },
        "object_id" : {
          "type" : "string",
          "example" : "ncbigene:8291"
        },
        "association_score" : {
          "type" : "number",
          "example" : 53.2
        }
      }
    }
  },
  "host" : "virtserver.swaggerhub.com",
  "basePath" : "/NCATS_GNBR/GNBR_API/1.0.0",
  "schemes" : [ "https" ]
}